{
  "name": "landy",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "cross-env NODE_ENV=development nodemon ./bin/www",
    "test": "cross-env NODE_ENV=production nodemon ./bin/www",
    "kill": "better-npm-run kill",
    "dev:dev": "better-npm-run dev:dev",
    "dev:prod": "better-npm-run dev:prod",
    "build:clean": "better-npm-run build:dev",
    "build:dev": "better-npm-run build:dev",
    "build:prod": "better-npm-run build:prod",
    "release:dev": "better-npm-run release:dev",
    "release:prod": "better-npm-run release:prod"
  },
  "betterScripts": {
    "dev:dev": {
      "command": "cross-env NODE_ENV=development gulp dev",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "dev:prod": {
      "command": "cross-env NODE_ENV=production gulp prod",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build:clean": {
      "command": "gulp clean"
    },
    "build:dev": {
      "command": "cross-env NODE_ENV=development gulp build-dev",
      "env": {
        "NODE_ENV": "development"
      }
    },
    "build:prod": {
      "command": "cross-env NODE_ENV=production gulp build-prod",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "kill": {
      "command": "gulp kill"
    },
    "release:dev": {
      "command": "cross-env NODE_ENV=development PORT=8080 pm2 list && pm2 reload ecosystem.config.js"
    },
    "release:prod": {
      "command": "cross-env NODE_ENV=production PORT=80 pm2 list && pm2 reload ecosystem.config.js --env production"
    }
  },
